{
	"tasks": {
		"dev": "deno run --watch main.ts"
	},
	"compilerOptions": {
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noUncheckedIndexedAccess": true
	},
	"fmt": {
		"options": {
			"useTabs": true,
			"lineWidth": 80,
			"singleQuote": true
		}
	},
	"imports": {
		"@b-fuze/deno-dom": "jsr:@b-fuze/deno-dom@^0.1.48",
		"@std/datetime": "jsr:@std/datetime@^0.225.2"
	}
}
